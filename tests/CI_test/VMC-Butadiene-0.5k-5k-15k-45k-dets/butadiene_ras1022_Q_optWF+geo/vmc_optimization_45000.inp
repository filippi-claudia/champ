%module general
    title        'butadiene vmc optimization 46000 dets'
    pool         'pool/'
    pseudopot    BFD
    basis        BFD-Q
    mode         vmc
%endmodule

load molecule        $pool/butadiene.xyz
load basis_num_info  $pool/BFD-Q.bfinfo

load determinants    det_optimal.1_start
load orbitals        orbitals_optimal.1_start
load jastrow         jastrow_optimal.1_start
load jastrow_der     jastrow.der
load symmetry        ci1010+SD_pVQZ_SOSCF.sym


%module electrons
    nup           11
    nelec         22
%endmodule

%module optwf
    ioptwf        1
    ioptci        1
    ioptjas       1
    ioptorb       1
    method        'sr_n'
    multiple_adiag 0
    ncore         0
    nextorb       500
    no_active     0
    nblk_max      100
    nopt_iter     2
    sr_tau        0.03d0
    sr_eps        0.001d0
    sr_adiag      0.01d0
    isample_cmat  0
    energy_tol    0.0d0
%endmodule

%module optgeo
    iforce_analy  1
    iuse_zmat     0
    alfgeo        0.25d0
%endmodule

%module vmc
    node_cutoff   1
    enode_cutoff  0.01d0
%endmodule

%module blocking_vmc
    vmc_nstep     20
    vmc_nblk      100
    vmc_nblkeq    1
    vmc_nconf_new 0
%endmodule

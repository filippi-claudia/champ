%module general
    title           'psb-20'
    pool            './pool/'
    pseudopot       BFD
    basis           bfd-dz
    weights         [ 1.0d0,  1.0d0 ]
    weights_guiding [ 1.0d0,  1.0d0 ]
    mode            'vmc_one_mpi1'
%endmodule

%module mstates
    iguiding        2
    iefficiency     1
%endmodule

load molecule        $pool/casci44-dz.geometry.xyz
load basis_num_info  $pool/bfd-dz.bfinfo

load determinants    casci44-dz.det.1
load orbitals        casci44-dz.lcao
load jastrow         jastrow_initial
load jastrow_der     jastrow.der
load symmetry        casci44-dz.sym


%module electrons
    nup           11
    nelec         22
%endmodule

%module optwf
    ioptwf        1
    ioptci        1
    ioptjas       1
    ioptorb       1
    method        'mix_n'
    multiple_adiag 0
    ncore         0
    nextorb       86
    no_active     0
    nblk_max      200
    nopt_iter     2
    isample_cmat  0
    energy_tol    0.0
    sr_tau      0.025
    sr_eps      0.001
    sr_adiag    0.05
    lin_eps     0.001
    lin_adiag   0.01
    lin_nvec    5
    lin_nvecx   20
    micro_iter_sr 3
%endmodule

%module blocking_vmc
    vmc_nstep     20
    vmc_nblk      200
    vmc_nblkeq    1
    vmc_nconf_new 0
%endmodule

%module properties
    sample        1
%endmodule

%module general
    title           'Benzene RHF calc cc-VDZ basis converted using gamess-trexio-champ converter'
    pool            './pool/'
    pseudopot       BFD
    basis           cc-VDZ
    mode            'vmc_one_mpi1'
%endmodule


load molecule        $pool/champ_v2_gamess_benzene_hf_cc-VDZ_geom.xyz

# These two files are taken from the original gamess2qmc converter
load basis_num_info  $pool/champ_v2_gamess_benzene_hf_cc-VDZ.bfinfo
load orbitals        champ_v2_gamess_benzene_hf_cc-VDZ_orbitals.lcao

load determinants    champ_v2_gamess_benzene_hf_cc-VDZ_determinants.det
load symmetry        champ_v2_gamess_benzene_hf_cc-VDZ_symmetry.sym
load jastrow         jastrow.jas

%module electrons
    nup           15
    nelec         30
%endmodule


%module blocking_vmc
    vmc_nstep     20
    vmc_nblk      1000
    vmc_nblkeq    1
    vmc_nconf_new 0
    kref_fixed    1
%endmodule


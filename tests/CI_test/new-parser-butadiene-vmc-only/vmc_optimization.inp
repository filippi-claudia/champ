#%module general
    title        'butadiene'
    pool         'pool/'
    pseudopot    BFD
    basis        BFD-T-normf0
    mode         vmc
    weights      [ 1.0d0,  1.0d0 , 1.0d0]
#%endmodule

load molecule        butadiene.xyz
load determinants    TZ_1M_500.det
load orbitals        TZ_1M_15k.orb
load basis_num_info ./pool/BFD-T.bfinfo
load jastrow         jastrow_optimal.0
load jastrow_der     jastrow.der
load symmetry        cas1010.sym


#%module electrons
    nup           11
    nelec         22
#%endmodule

#%module jastrow
    ianalyt_lap    1
    ijas           4
    isc            2
    nspin1         1
    nspin2         1
    ifock          0
#%endmodule

#%module optwf
    ioptwf        1
    ioptci        1
    ioptjas       1
    ioptorb       1
    method        'sr_n'
    multiple_adiag 0
    ncore         0
    nextorb       280
    nblk_max      200
    no_active     0
    nopt_iter     2
    sr_tau        0.025
    sr_eps        0.001
    sr_adiag      0.01
    isample_cmat  0
    energy_tol    0.0
#%endmodule

#%module blocking_vmc
    vmc_nstep     20
    vmc_nblk      50
    vmc_nblkeq    1
    vmc_nconf_new 0
    vmc_isite     0
#%endmodule

#%module blocking_dmc
    dmc_nstep     60
    dmc_nblk      10
    dmc_nblkeq    1
    dmc_nconf     20
    dmc_nconf_new 0
#%endmodule

#%module dmc
    tau           0.05
    etrial      -26.3d0
    icasula      -1
#%endmodule